{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "flashDB",
    "public": true
  },
  "AccessToken": {
    "dataSource": "flashDB",
    "public": false
  },
  "ACL": {
    "dataSource": "flashDB",
    "public": true
  },
  "RoleMapping": {
    "dataSource": "flashDB",
    "public": false
  },
  "Role": {
    "dataSource": "flashDB",
    "public": true,
    "$options": {
      "base": "Role",
      "relations": {
        "principals": {
          "type": "hasMany",
          "model": "roleMapping",
          "foreignKey": "roleId"
        }
      }
    }
  },
  "pages": {
    "dataSource": "flashDB",
    "public": true,
    "$promise": {},
    "$resolved": true
  },
  "sites": {
    "dataSource": "flashDB",
    "public": true,
    "$promise": {},
    "$resolved": true
  },
  "settings": {
    "dataSource": "flashDB",
    "public": true
  },
  "uploads": {
    "dataSource": "container",
    "public": true
  }
}
