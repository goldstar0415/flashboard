      "type": "string",
      "id": true,
      "index": true
    },
    "title": {
      "label":"Page Title",
      "type": "string",
      "required": true,
      "defaultColumn":true
    },
    "site": {
      "type": "string",
      "required": true
    },
    "transition": {
      "type": "string",
      "default": "default"
    },
    "html": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "default": "1"
    },
    "site_name": {
      "type": "string",
      "defaultColumn":true,
      "label": "Site Name"
    },
    "variant_data": {
      "type": "string",
      "id": false,
      "required": false
    },
    "description": {
      "type": "string",
      "id": false,
      "index": false
    },
    "slug": {
      "type": "string",
